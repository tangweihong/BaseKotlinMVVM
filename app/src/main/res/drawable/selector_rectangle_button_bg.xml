<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <!--<color android:color="@color/red_press" />-->
        <shape android:shape="rectangle">
            <solid android:color="#FFFABE00" />
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
        </shape>
    </item>
    <item android:state_selected="true">
        <!--<color android:color="@color/red_press" />-->
        <shape android:shape="rectangle">
            <solid android:color="#FFFABE00" />
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <!--<color android:color="#ec8686" />-->
        <shape android:shape="rectangle">
            <solid android:color="#FFFABE00" />
            <corners android:radius="5dp" />
            <!--<stroke android:width="1dp" android:color="#000000" />-->
        </shape>
    </item>
    <item>
        <!--<color android:color="@color/red" />-->
        <shape android:shape="rectangle">
            <solid android:color="#FFFABE00" />
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
        </shape>
    </item>
</selector>